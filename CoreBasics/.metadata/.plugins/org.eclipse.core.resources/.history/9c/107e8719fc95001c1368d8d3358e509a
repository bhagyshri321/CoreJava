
public class SetGetter {
	
	private String name;
	private String  add;
	private int age;
	
	public SetGetter() {}
	
	public SetGetter (String n, String a,  int ag) {
		String name = n;
		String add= a;
        int  age= ag;     }

	public String getName() {
		return name;
	}

	

	public String getAdd() {
		return add;
	}

	

	public int getAge() {
		return age;
	}}
	
		
	

