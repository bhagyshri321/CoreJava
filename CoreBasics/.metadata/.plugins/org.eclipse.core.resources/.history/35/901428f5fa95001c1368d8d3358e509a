
public class SetGetter {
	
	private String name;
	private String  add;
	private int age;
	
	public SetGetter() {}
	
	public SetGetter (String n, String a,  int ag) {
		String name = n;
		String add= a;
        int  age= ag;     }

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAdd() {
		return add;
	}

	public void setAdd(String add) {
		this.add = add;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	
	
	
		
	
	}
	
		
	

